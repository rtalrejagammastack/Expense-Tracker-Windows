<%= form_with model: user_category do |form| %>
  <div class="form-outline mb-4 w-25">
      <%= form.text_field :name,class:'form-control form-control-lg' %>
      <%= form.label :name,class: 'form-label' %>
  </div>
  <div class="form-group mb-4 w-25">
      <%= form.label :background, "Background Image", class: 'form-label' %>
      <%= form.file_field :background %>
  </div>
  <%= form.submit class:"btn btn-primary btn-rounded shadow btn-lg" %>
<% end %>